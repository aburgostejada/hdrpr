<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".Dashboard$PlaceholderFragment">

    <!--<TextView android:id="@+id/section_label" android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:text="@string/dashboard"-->
        <!--android:textColor="#ff2a2936"-->
        <!--android:textStyle="bold"-->
        <!--android:textSize="14pt"-->
        <!--android:typeface="sans" />-->




    <FrameLayout android:id="@+id/process_image_fragment_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal|bottom"
        android:layout_above="@+id/dashboardLabel"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="25sp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/dashboardLabel"
        android:textSize="25sp"
        android:layout_above="@+id/processImage"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="20sp" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/processButton"
        android:id="@+id/processImage"
        android:clickable="true"
        android:padding="10pt"
        android:textSize="20pt"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />




</RelativeLayout>
